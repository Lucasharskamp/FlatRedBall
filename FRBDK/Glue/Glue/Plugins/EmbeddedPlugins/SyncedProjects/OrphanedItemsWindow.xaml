<Window x:Class="FlatRedBall.Glue.Controls.ProjectSync.OrphanedItemsWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:local="clr-namespace:FlatRedBall.Glue.Controls.ProjectSync"
             xmlns:controls="clr-namespace:GlueFormsCore.Controls"
             xmlns:localization="clr-namespace:Localization;assembly=Localization"
             Width="300" Height="300" 
             Title="{x:Static localization:Texts.ItemsOrphaned}"
             Style="{Binding Source={x:Static controls:MainPanelControl.CustomWindowStyle}}">
    <Grid>
        <local:OrphanedItemsControl></local:OrphanedItemsControl>     
    </Grid>
</Window>
